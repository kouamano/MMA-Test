   3.4103878066489573`*^9}, {3.410392684*^9, 3.410392716625*^9}, {
   3.4105329504275985`*^9, 3.410532953380723*^9}, {3.410534982536973*^9, 
   3.410534989286973*^9}, {3.585934558687821*^9, 3.585934564550495*^9}},
 CellID->1626763557],

